<h1>All Users</h1>

<ul id="user_index">
<% @users.each do |user| %>
	<li>
    <%= link_to image_tag(user.photo.thumb), user_path(user) %>
    <div class='id'><i class="icon-user"></i> <%= link_to user.name, user_path(user) %></div>

  </li>
<% end %>
</ul>

<%= paginate @users %>
<%= link_to "Create New User", new_user_path %>